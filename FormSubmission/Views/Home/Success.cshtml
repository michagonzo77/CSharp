@model User

<main class="d-flex justify-content-center">
    <div class="mt-5 border border-dark w-25 p-5">
        <p>Name: @Model.Name</p>
        <p>Email: @Model.Email</p>
        <p>Birthday: @Model.Date</p>
        <p>Password: @Model.Password</p>
        <p>Favorite Odd Number: @Model.Favorite</p>
    </div>
</main>
